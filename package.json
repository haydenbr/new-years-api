{
    "name": "new-years-api",
    "version": "0.1.0",
    "description": "Quick and dirty now. Make it nice later.",
    "license": "MIT",
    "repository": "https://github.com/haydenbr/new-years-api",
    "dockerHubRepository": "haydenbr/new-years-api",
    "main": "./src/app.js",
    "scripts": {
        "start": "docker-compose -f ../docker-compose.dev.yml -f ./docker-compose.dev.yml up",
        "down": "docker-compose -f ../docker-compose.dev.yml -f ./docker-compose.dev.yml down",
        "bump": "./scripts/bump.js",
        "docker-build": "docker build -t $(./scripts/current-tag.js) .",
        "docker-push": "docker push $(./scripts/current-tag.js)",
        "major": "yarn bump major",
        "minor": "yarn bump minor",
        "patch": "yarn bump patch",
        "serve": "nodemon --inspect=0.0.0.0:5858"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "compression": "^1.7.2",
        "cors": "^2.8.4",
        "express": "4.16.3",
        "method-override": "^2.3.10",
        "mongoose": "5.0.15",
        "mongoose-sequence": "^4.0.1",
        "morgan": "^1.9.0"
    },
    "devDependencies": {
        "@types/express": "^4.11.1",
        "@types/mongoose": "^5.0.10",
        "chalk": "^2.4.1",
        "js-yaml": "^3.11.0",
        "nodemon": "^1.17.3"
    }
}